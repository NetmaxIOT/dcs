<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-dcs">
    <!--<sub-class-of type="application/xml"/>-->
    <comment xml:lang="en">OpenDCS Configuration</comment>
    <generic-icon name="image-x-generic"/>
    <magic priority="50">
      <match value="&lt;dcs&gt;" type="string" offset="0"/>
    </magic>
    <glob pattern="*.dcs"/>
    <glob pattern="*.dcf"/>
    <glob pattern="*.DCF"/>
    <icon name="dcs"/>
  </mime-type>
</mime-info>
